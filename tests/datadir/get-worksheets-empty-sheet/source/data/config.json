{
  "authorization": {
    "oauth_api": {
      "credentials": {
        "#data": "${OAUTH_DATA}",
        "#appSecret": "${OAUTH_APP_SECRET}",
        "appKey": "${OAUTH_APP_ID}"
      }
    }
  },
  "action": "getWorksheets",
  "parameters": {
    "workbook": {
      "driveId": "${FIXTURES_CATALOG::DRIVE::FILE_EMPTY::getDriveId}",
      "fileId": "${FIXTURES_CATALOG::DRIVE::FILE_EMPTY::getFileId}"
    }
  }
}
